# Gets the loc to be displayed based on what the current v (value) of the element in the array is (the value corresponds to the party id set in the scripted effect).
defined_text = {
	name = GET_fra_nord_prov
	text = {
		trigger = {
			29 = {
				check_variable = {
					var = FRA_mob_limit
					value = 1
					compare = less_than
				}
				is_owned_by = FRA
			}
		}
		localization_key = "GFX_FRA_mob_limit_nord_icon_0"
	}
	text = {
		trigger = {
			29 = {
				check_variable = {
					var = FRA_mob_limit
					value = 1
					compare = equals
				}
				is_owned_by = FRA
			}
		}
		localization_key = "GFX_FRA_mob_limit_nord_icon_1"
	}
}

